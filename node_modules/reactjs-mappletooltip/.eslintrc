{
  "parser": "babel-eslint",
  "rules": {
    "no-console":0,
    "no-alert":0,
    "indent": [
      2,
      2
    ],
    "quotes": [
      2,
      "single"
    ],
    "babel/object-curly-spacing": [2, "always"],
    "object-curly-spacing": [2, "always"],
    "babel/object-shorthand": [0, "always"],
    "babel/arrow-parens": [0, "always"],
    "semi": [
      2,
      "always"
    ],
    "no-reserved-keys": 0,
    "no-wrap-func": 0,
    "spaced-line-comment": 0,
    "func-names": 0,
    "no-nested-ternary": 0,
    "react/sort-comp": [1, {
      "order": [
        "constructor",
        "lifecycle",
        "render",
        "everything-else"
      ]
    }],
    "react/no-multi-comp": 1,
    "id-length": [1, {
      "exceptions": ["e", "_"]
    }]
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "extends": ["airbnb", "airplus"],
  "ecmaFeatures": {
    "jsx": true,
    "experimentalObjectRestSpread": true
  },
  "plugins": [
    "react"
  ],
  "globals": {
    "__DEVTOOLS__": true,
    "__ENV__": true,
    "describe": true,
    "it": true,
    "afterEach": true,
    "beforeEach": true,
    "jasmine": true
  }
}